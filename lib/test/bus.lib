      }
      pin(b1) {
        direction: out;
        timing() {
          timing_sense : negative_unate;
          related_pin : "in"
          ; f f
          (
          ) 
          ;
          f : g; g
          : g
          f : t y : h
          fall_transition(scalar){
             values("42") ;
          }
        }
      } /* pin */
    } /* bus */
  } /* cell */
} /* library test.db */

